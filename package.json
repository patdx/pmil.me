{
  "name": "@patdx/site",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy",
    "dev": "remix vite:dev",
    "start": "wrangler pages dev",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "update-react": "pnpm install --save-exact react@rc react-dom@rc",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "format": "biome format --write",
    "db-gen": "drizzle-kit generate",
    "db-up": "pnpm wrangler d1 migrations apply pmil-me",
    "db-exec": "pnpm wrangler d1 execute pmil-me"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.11.0",
    "@remix-run/cloudflare-pages": "2.11.0",
    "@remix-run/react": "2.11.0",
    "@tryfabric/martian": "1.2.4",
    "@unpic/react": "0.1.14",
    "clsx": "2.1.1",
    "drizzle-orm": "0.32.1",
    "humanize-url": "3.0.0",
    "isbot": "5.1.13",
    "lodash-es": "4.17.21",
    "path-browserify": "1.0.1",
    "react": "rc",
    "react-dom": "rc",
    "react-loops": "1.3.0",
    "react-notion-x": "6.16.0",
    "rehype-sanitize": "6.0.0",
    "rehype-stringify": "10.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "safe-mdx": "0.0.4",
    "sharp": "0.33.4",
    "solid-js": "1.8.19",
    "tailwindcss": "3.4.7",
    "ulidx": "2.4.0",
    "unified": "11.0.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "4.20240729.0",
    "@iconify-json/ion": "1.1.18",
    "@libsql/client": "0.8.1",
    "@mdx-js/rollup": "3.0.1",
    "@notionhq/client": "2.2.15",
    "@remix-run/dev": "2.11.0",
    "@shikijs/rehype": "1.12.1",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.13",
    "@types/lodash-es": "4.17.12",
    "@types/path-browserify": "1.0.2",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.23.1",
    "gray-matter": "4.0.3",
    "postcss": "8.4.40",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "5.0.0",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4",
    "unplugin-auto-import": "0.18.2",
    "unplugin-icons": "0.19.1",
    "vite": "5.3.5",
    "vite-imagetools": "7.0.4",
    "vite-tsconfig-paths": "4.3.2",
    "wrangler": "3.68.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    }
  },
  "packageManager": "pnpm@9.6.0"
}
