{
  "name": "@patdx/site",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy",
    "dev": "remix vite:dev",
    "start": "wrangler pages dev",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "update-react": "pnpm install --save-exact react@rc react-dom@rc",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "format": "biome format --write",
    "db-gen": "drizzle-kit generate",
    "db-up": "pnpm wrangler d1 migrations apply pmil-me",
    "db-exec": "pnpm wrangler d1 execute pmil-me"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.11.2",
    "@remix-run/cloudflare-pages": "2.11.2",
    "@remix-run/react": "2.11.2",
    "@tryfabric/martian": "1.2.4",
    "@unpic/react": "0.1.14",
    "clsx": "2.1.1",
    "drizzle-orm": "0.33.0",
    "humanize-url": "3.0.0",
    "isbot": "5.1.17",
    "lodash-es": "4.17.21",
    "path-browserify": "1.0.1",
    "react": "rc",
    "react-dom": "rc",
    "react-loops": "1.3.0",
    "react-notion-x": "6.16.0",
    "rehype-sanitize": "6.0.0",
    "rehype-stringify": "10.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "safe-mdx": "0.0.4",
    "sharp": "0.33.5",
    "solid-js": "1.8.22",
    "tailwindcss": "3.4.10",
    "ulidx": "2.4.1",
    "unified": "11.0.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "4.20240821.1",
    "@iconify-json/ion": "1.1.19",
    "@libsql/client": "0.10.0",
    "@mdx-js/rollup": "3.0.1",
    "@notionhq/client": "2.2.15",
    "@remix-run/dev": "2.11.2",
    "@shikijs/rehype": "1.15.2",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.15",
    "@types/lodash-es": "4.17.12",
    "@types/path-browserify": "1.0.3",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.24.2",
    "gray-matter": "4.0.3",
    "postcss": "8.4.42",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "5.0.0",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "unplugin-auto-import": "0.18.2",
    "unplugin-icons": "0.19.2",
    "vite": "5.4.2",
    "vite-imagetools": "7.0.4",
    "vite-tsconfig-paths": "5.0.1",
    "wrangler": "3.73.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    }
  },
  "packageManager": "pnpm@9.9.0"
}
